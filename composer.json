{
  "name": "railroad/event-data-synchronizer",
  "type": "library",
  "description": "This package has reconfigured event listeners for syncing data across packages.",
  "keywords": [
    "event",
    "data",
    "synchronizer",
    "sync"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "railroad/railcontent": "2.0.0",
    "railroad/ecommerce": "5.0.*",
    "railroad/usora": "3.0.*",
    "railroad/customer-io": "3.0.*",
    "railroad/railhelpscout": "2.0.*",
    "railroad/railforums": "2.0.*",
    "railroad/railanalytics": "3.0.*",
    "railroad/referral": "3.0.*",
    "railroad/points": "dev-v1.0-php-8-laravel-9-migration"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\EventDataSynchronizer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\EventDataSynchronizer\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
