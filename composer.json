{
  "name": "railroad/event-data-synchronizer",
  "type": "library",
  "description": "This package has reconfigured event listeners for syncing data across packages.",
  "keywords": [
    "event",
    "data",
    "synchronizer",
    "sync"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/maropost"
    }
  ],
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "railroad/railcontent": "0.5.*",
    "railroad/ecommerce": "2.4.*",
    "railroad/usora": "2.0.*",
    "railroad/intercomeo": "2.0.*",
    "railroad/maropost": "1.0.*",
    "railroad/customer-io": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\EventDataSynchronizer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\EventDataSynchronizer\\Tests\\": "tests/"
    }
  }
}